/ {
 compatible = "cubietech,cubieboard6", "actions,s500";
 model = "CubieBoard6";

 aliases {
  serial3 = &uart3;
 };

 chosen {
  stdout-path = "serial3:115200n8";
 };

 memory@0 {
  device_type = "memory";
  reg = <0x0 0x80000000>;
 };

 uart3_clk: uart3-clk {
  compatible = "fixed-clock";
  clock-frequency = <921600>;
  #clock-cells = <0>;
 };
};

&timer {
 clocks = <&hosc>;
};

&uart3 {
 status = "okay";
 clocks = <&uart3_clk>;
};
